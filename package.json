{
  "name": "socatar",
  "version": "1.0.0",
  "description": "Web service for easily requesting profile photos from several sources",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "start-dev": "nodemon src/server.js --watch src/",
    "start-dev-managed": "set TIER=managed&&nodemon src/server.js --watch src/",
    "test": "standard && ava",
    "test-managed": "set TIER=managed&& standard && ava",
    "fix-style": "standard --fix"
  },
  "dependencies": {
    "cloudinary": "1.9.1",
    "dotenv": "4.0.0",
    "express": "4.16.2",
    "lodash.get": "4.4.2",
    "lodash.set": "4.3.2",
    "opbeat": "4.16.0",
    "request": "2.83.0",
    "streamsearch": "0.1.2"
  },
  "devDependencies": {
    "ava": "0.24.0",
    "codacy-coverage": "2.0.3",
    "coveralls": "3.0.0",
    "jpeg-js": "0.3.3",
    "nodemon": "1.14.9",
    "nyc": "11.4.1",
    "pixelmatch": "4.0.2",
    "pngjs": "3.3.1",
    "rimraf": "2.6.2",
    "standard": "10.0.3",
    "supertest": "3.0.0"
  },
  "engines": {
    "node": "8.9.3"
  },
  "author": "Aleksander Terekhov <socatar.com@gmail.com> (https://www.terales.info/)",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/terales/socatar"
  }
}
